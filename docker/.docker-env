# APP
COMPOSER_BITBUCKET_KEY=
COMPOSER_BITBUCKET_SECRET=
COMPOSER_GITHUB_TOKEN=
COMPOSER_MAGENTO_PASSWORD=
COMPOSER_MAGENTO_USERNAME=
COMPOSER_VERSION=1.10.16
ENABLE_SSMTP=1
ENABLE_XDEBUG=1
MAGENTO_RUN_MODE=developer
WEB_GROUP=www-data
WEB_HOST=localhost
WEB_ROOT=/www
WEB_USER=www-data
USER_DIRECTORY=/root

# DB
MYSQL_DATABASE=magento
MYSQL_PASSWORD=magento
MYSQL_ROOT_PASSWORD=magento
MYSQL_USER=magento

# EMAIL
EMAIL_ADDRESS=user@domain.tld
EMAIL_PASSWORD=
EMAIL_USERNAME=
SMTP_HOST=
SMTP_PORT=

# FTSE
discovery.type=single-node
ES_JAVA_OPTS="-Xms512m -Xmx512m"

# IMDB
REDIS_DSN=redis://mage.imdb
REDIS_HOST=redis://mage.imdb
REDIS_PASSWORD=redis
REDIS_PORT=6379
REDIS_REPLICATION_MODE=master

# MB
RABBITMQ_PASSWORD=rabbitmq
RABBITMQ_HOST=amqp://rabbitmq:rabbitmq@mage.mb
RABBITMQ_URL=amqp://rabbitmq:rabbitmq@mage.mb:5672/%2F?lazy=1&connection_timeout=2
RABBITMQ_PORT=5672
RABBITMQ_USER=rabbitmq
